					<tbody class="u-align-center u-table-body u-text-body-color">
						<c:forEach items="${site.tours}" var="tour">

							<tr style="height: 80px;">
								<td class="u-first-column u-table-cell u-white"><c:out
										value="${tour.site.name}"></c:out></td>
								<td class="u-table-cell"><c:out value="${tour.capacity }"></c:out></td>
								<td class="u-table-cell u-white u-table-cell-8"><c:out
										value="${tour.fees }"></c:out></td>
								<td class="u-table-cell"><fmt:formatDate
										value="${tour.date}" pattern="yyyy-MM-dd" /></td>
								<td class="u-table-cell"><c:choose>
										<c:when test="${ tour.travellers.contains(user) }">
											<span>Joining <a href="/tours/${ tour.id }/a/cancel">Cancel</a></span>
										</c:when>
										<c:otherwise>
											<a href="/tours/${ tour.id }/a/join">Join</a>
										</c:otherwise>

									</c:choose></td>
							</tr>
						</c:forEach>

					</tbody>
				</table>
				<br>
				<h3>Reviews</h3>
				<c:forEach items="${ site.reviews }" var="review">
					<p>${ review.siteU.username } says: ${ review.content }</p>
				</c:forEach>
				<form action="/${ site.id }/comment" method="post">
					<div>
						<label for="comment">Add review</label> <span>${ error }</span><br><br>
						<textarea name="comment" id="comment" style="width: 300px;"></textarea><br><br>
						<button class="btn btn-primary">Submit</button>
					</div>
				</form>
			</div>
		</div>
	</section>


